# Root-level (Add a private-key in the .pem format and the identity provider's public key in the .crt format to the same directory)
privateKeyPath = "./<your-superapp-private-key>.pem"
publicKeyPath = "./<your-idp-certificate>.crt"

# Optional (for direct HTTPS from clients to backend)
selfSignedCertFile = "./server-cert.crt"  
selfSignedKeyFile  = "./server-key.key"

[databaseConfig]
host = "<your-db-host-name>"
user = "<your-db_admin-name>"
password = "<your-db-password>"
database = "<your-db-name>"
port = <port>